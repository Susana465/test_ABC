begin model

begin parameters
  A_i 100
  B_i 150


  kon 1e-2
  koff 1e-3

  ITERATIONS 3e2

end parameters

begin molecule types
    A()
    B()
    C()
end molecule types

begin compartments
end compartments

begin species
    A() A_i 
    B() B_i
end species

begin observables
    Molecules A A()
    Molecules B B()
    Molecules C C()
end observables


begin reaction rules
    A() + B() <-> C() kon, koff
end reaction rules

end model
simulate({method=>"ssa",t_end=>"ITERATIONS",n_steps=>100})