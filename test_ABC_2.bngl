begin parameters
    MCELL_DIFFUSION_CONSTANT_3D_A 1e-6
    MCELL_DIFFUSION_CONSTANT_2D_B 1e-6
    MCELL_DIFFUSION_CONSTANT_3D_C 1e-6
    kon222 1e8
    ITERATIONS 1e2
    VolCP 0.50588 
    VolPM 0.0307 # from volume of 0.505 r = 0.494 and area = 3.07um2 * 0.01um thickness. Assuming a thickness of 0.01 microns for the PM
end parameters

begin compartments
    PM 2 VolPM #2Dimensions, surface compartment
 	CP 3 VolCP PM #3Dimensions and 1 microcubic in volume, volume compartment
end compartments

begin seed species
    A(b)@CP 100
    B(a)@PM 100
    C()@CP 0
end seed species  

begin observables
    Molecules A A(b)
    Molecules B B(a)
    Molecules AB B(a)
    Molecules C C()
end observables

begin reaction rules
    A(b)@IN + B(a) -> A(b!1).B(a!1) kon222
    #A(b)@IN + B(a) -> C()@IN kon222
end reaction rules  

simulate({method=>"ssa",t_end=>"ITERATIONS",n_steps=>10})